{
  "name": "@packages/db",
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "tsc --watch",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "postinstall": "prisma generate",
    "build": "tsc",
    "lint": "TIMING=1 eslint \"**/*.{ts,tsx,js,jsx}\" --fix",
    "clean": "rm -rf ./dist && rm -rf .turbo"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./*": [
      "./dist/*.js",
      "./dist/*/index.js"
    ]
  },
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "@packages/env": "workspace:*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "src/*",
        "src/*/index"
      ]
    }
  },
  "publishConfig": {
    "typesVersions": {
      "*": {
        "*": [
          "dist/*.d.ts",
          "dist/*/index.d.ts"
        ]
      }
    }
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/node": "^18.17.0",
    "eslint": "^8.11.0",
    "prisma": "^5.6.0",
    "typescript": "^4.5.5"
  }
}